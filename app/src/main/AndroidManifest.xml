<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.smu.cervicalcancerapp" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.smu.cervicalcancerapp.StartScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.smu.cervicalcancerapp.NewPatient"
            android:label="@string/title_activity_new_patient" >
        </activity>
        <activity
            android:name="edu.smu.cervicalcancerapp.SelectCurrentPatient"
            android:label="@string/title_activity_select_current_patient" >
        </activity>
        <activity
            android:name="edu.smu.cervicalcancerapp.PatientInfo"
            android:label="@string/title_activity_patient_info" >
        </activity>
        <activity
            android:name="edu.smu.cervicalcancerapp.TakeImage"
            android:label="@string/title_activity_take_image" >
        </activity>
        <activity
            android:name="edu.smu.cervicalcancerapp.KeepImage"
            android:label="@string/title_activity_keep_image" >
        </activity>
        <activity
            android:name="edu.smu.cervicalcancerapp.TakeAnotherImage"
            android:label="@string/title_activity_take_another_image" >
        </activity>
        <activity
            android:name="edu.smu.cervicalcancerapp.ViewImage"
            android:label="@string/title_activity_view_image" >
        </activity>
        <activity
            android:name="edu.smu.cervicalcancerapp.ProcessingDiagnosis"
            android:label="@string/title_activity_processing_diagnosis" >
        </activity>
    </application>

</manifest>
